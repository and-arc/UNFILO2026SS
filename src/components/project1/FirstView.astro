---
import Item from './parts/Item.astro';
---

<!-- Key Visual Section -->
<div class="fv">
  <picture class="fv_image">
    <img src="./img/fv_img01_sp.jpg" alt="">
  </picture>

  <!-- 横スクロールタイトル -->
  <div class="fv_titleWrapper">
    <div class="swiper fv_titleSwiper">
      <div class="swiper-wrapper">
        <div class="swiper-slide">
          <img src="./img/fv_text01.svg" alt="( 2026 Spring ) UNF/LO magazine Feat.Mi" class="fv_titleImage">
        </div>
        <div class="swiper-slide">
          <img src="./img/fv_text01.svg" alt="" aria-hidden="true" class="fv_titleImage">
        </div>
        <div class="swiper-slide">
          <img src="./img/fv_text01.svg" alt="" aria-hidden="true" class="fv_titleImage">
        </div>
        <div class="swiper-slide">
          <img src="./img/fv_text01.svg" alt="" aria-hidden="true" class="fv_titleImage">
        </div>
				 <div class="swiper-slide">
          <img src="./img/fv_text01.svg" alt="" aria-hidden="true" class="fv_titleImage">
        </div>
				<div class="swiper-slide">
          <img src="./img/fv_text01.svg" alt="" aria-hidden="true" class="fv_titleImage">
        </div>
      </div>
    </div>
  </div>
</div>

<!-- Introduction Section -->
<div id="intro" class="section">
  <h2 class="title is-fadeup">
    <img src="./img/intro_title01.png" alt="ラッシュの春はアンフィーロの万能服に救われたい">
  </h2>
  <!-- 商品ラインナップ -->
  <div class="item">
    <Item themeId={1} />
  </div>
</div>
